{
  "name": "redhead",
  "description": "Dynamically setup headers and redirects for you static deployments",
  "version": "0.3.2",
  "bin": {
    "redhead": "./bin/run"
  },
  "bugs": "https://github.com/streaver/redhead/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "chalk": "^2.4.2",
    "fs-extra": "^7.0.1",
    "rimraf": "^2.6.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "chai": "^4",
    "eslint": "5.3.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-oclif": "^3.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.0",
    "globby": "^9",
    "mocha": "^6",
    "mock-fs": "^4.8.0",
    "nyc": "^13"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/streaver/redhead",
  "keywords": [
    "headers",
    "redirects",
    "netlify",
    "dynamic"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "redhead",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "streaver/redhead",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "nyc --reporter lcov --exclude .redhead/**/*.js mocha --forbid-only \"test/**/*.test.js\"",
    "report": "nyc report",
    "version": "oclif-dev readme && git add README.md"
  }
}
